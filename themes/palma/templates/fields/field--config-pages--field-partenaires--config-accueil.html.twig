<section class="join_us relativen border-bottom"> 
     <div class="container partner p-0 d-flex   flex-column">
       <div class="row   flex-fill align-items-center justify-content-center">
        <div class="col-lg-12 join-us__box text-center">
             <h3 class="join_us__box_title box_title text semi-bold">
             {{'Nos partenaires'|t}}  
            </h3> 
             {% if items is not null and items|length > 4 %}
                    <div class="flexslider_1">
                         <ul class="slides">  
                            {% for item in items %}
                                <li>
                                    {{ item.content }}
                                </li>
                              {% endfor %} 
                        </ul>
                      </div>  
                      {% else %}
                        {% for item in items %}
                            {{ item.content  }}
                        {% endfor %}                                                           
              {% endif %} 
      </div>
    </div>
  </div>
</section>
 
   
         