
<!--Palma (Header)  -->  
<main id="site">   
    <header class="header desktop">
      <div class="container">
         <div class="row align-items-center justify_content">
       
             {% if page.header %}
                 {{ page.header}}
             {% endif %} 
             
                   {% if page.primary_menu %}
                         {{ page.primary_menu  }}       
                   {% endif %}

              {% if page.header_right %}
                    {{page.header_right}}
              {% endif %}

        </div>
    </div>
</header>

 
 {% if  not is_front %}
     <div class="main_wrapper">
        <div class="main_container"> 
            {% if page.breadcrumb  %}
                <div class="container pl_2">
                  <div class="page_title_inner">
                        <div class="breadcrumb">
                          {{ page.breadcrumb }} 
                        </div>
                  </div>
                </div>
            {% endif %}  
        </div>
             
         <div class="content_interne container">
                 {{ page.content }}         
         </div>
    </div>
{% endif %}


<a href="javascript:void(0)" id="back_top">
    <i class="fa fa-angle-up"></i>
</a>


  <!--\Palma\(Slider\Carousel-Categorie\Filter-Populaire\Partenaire)-->
   {% if page.homepage_content %}
         {{ page.homepage_content  }}       
   {% endif %}

   {% if page.newsletters %}
         {{ page.newsletters  }}       
   {% endif %}  


<!--\Palma\(Newsletter\logo et reseau social\footer)-->
   <footer class="footer">    
        {% if page.footer %}
                 {{ page.footer  }}       
         {% endif %} 
  </footer>
</main>